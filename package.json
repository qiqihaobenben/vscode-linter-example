{
      "name": "vscode-linter-example",
      "version": "1.0.0",
      "description": "代码规范和编码风格示例项目",
      "main": "index.js",
      "scripts": {
            "lint": "eslint .",
            "fix-lint": "eslint . --cache --fix",
            "format": "prettier --write src/",
            "prepare": "husky install",
            "babel-dev": "babel src --out-dir lib"
      },
      "author": "qiqihaobenben",
      "license": "ISC",
      "devDependencies": {
            "@babel/cli": "^7.18.9",
            "@babel/core": "^7.18.9",
            "@babel/generator": "^7.18.9",
            "@babel/parser": "^7.18.9",
            "@babel/preset-env": "^7.18.9",
            "@babel/traverse": "^7.18.9",
            "@babel/types": "^7.18.9",
            "@commitlint/cli": "^17.0.3",
            "@commitlint/config-conventional": "^17.0.3",
            "browserslist": "^4.21.2",
            "cz-git": "^1.3.9",
            "cz-relax": "^0.2.1",
            "eslint": "^8.20.0",
            "eslint-config-prettier": "^8.5.0",
            "eslint-plugin-prettier": "^4.2.1",
            "husky": "^8.0.1",
            "lint-staged": "^13.0.3",
            "prettier": "^2.7.1"
      },
      "lint-staged": {
            "*.js": "eslint --cache --fix"
      },
      "config": {
            "commitizen": {
                  "path": "./node_modules/cz-git"
            }
      },
      "browserslist": [
            "> 0.2%",
            "last 2 versions",
            "not dead",
            "Firefox ESR"
      ],
      "dependencies": {
            "core-js": "^2.6.12"
      }
}
